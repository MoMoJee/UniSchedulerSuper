{
    "_comment": "API 密钥配置模板 - 复制此文件为 api_keys.json 并填入实际密钥",
    
    "llm": {
        "_description": "大语言模型 API 密钥",
        "deepseek": {
            "api_key": "your-deepseek-api-key-here",
            "base_url": "https://api.deepseek.com",
            "default_model": "deepseek-chat"
        },
        "moonshot": {
            "api_key": "your-moonshot-api-key-here",
            "base_url": "https://api.moonshot.cn/v1",
            "default_model": "kimi-latest"
        },
        "openai": {
            "api_key": "your-openai-api-key-here",
            "base_url": "https://api.openai.com/v1",
            "default_model": "gpt-4-turbo"
        }
    },
    
    "map_services": {
        "_description": "地图服务 API 密钥",
        "amap": {
            "api_key": "your-amap-key-here",
            "mcp_url": "https://mcp.amap.com/sse"
        }
    },
    
    "search_services": {
        "_description": "搜索服务 API 密钥",
        "tavily": {
            "api_key": "your-tavily-api-key-here"
        },
        "serper": {
            "api_key": "your-serper-api-key-here"
        }
    },
    
    "mcp_services": {
        "_description": "MCP 服务配置",
        "12306": {
            "enabled": true,
            "mcp_url": "http://localhost:8001/mcp",
            "transport": "streamable_http",
            "description": "12306 火车票查询服务"
        }
    },
    
    "other_services": {
        "_description": "其他服务 API 密钥"
    }
}
